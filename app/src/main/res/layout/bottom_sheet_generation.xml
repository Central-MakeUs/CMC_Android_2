<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/gray_900">

    <TextView
        android:id="@+id/bottom_sheet_generation_title_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="기수를 선택해주세요"
        android:textColor="@color/gray_500"
        android:textSize="18sp"
        android:lineHeight="17sp"
        android:fontFamily="@font/pretendard_bold"
        android:layout_marginTop="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/bottom_sheet_14th_cl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:paddingHorizontal="24dp"
        app:layout_constraintTop_toBottomOf="@id/bottom_sheet_generation_title_tv">

        <View
            android:id="@+id/bottom_sheet_14th_line"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/gray_800"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="14기"
            android:textColor="@color/gray_100"
            android:textSize="18sp"
            android:fontFamily="@font/pretendard_bold"
            android:paddingVertical="25dp"
            app:layout_constraintTop_toBottomOf="@id/bottom_sheet_14th_line"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/bottom_sheet_13th_cl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingHorizontal="24dp"
        app:layout_constraintTop_toBottomOf="@id/bottom_sheet_14th_cl">

        <View
            android:id="@+id/bottom_sheet_13th_line"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/gray_800"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="13기"
            android:textColor="@color/gray_100"
            android:textSize="18sp"
            android:fontFamily="@font/pretendard_bold"
            android:paddingVertical="25dp"
            app:layout_constraintTop_toBottomOf="@id/bottom_sheet_13th_line"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/bottom_sheet_12th_cl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingHorizontal="24dp"
        app:layout_constraintTop_toBottomOf="@id/bottom_sheet_13th_cl">

        <View
            android:id="@+id/bottom_sheet_12th_line"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/gray_800"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="12기"
            android:textColor="@color/gray_100"
            android:textSize="18sp"
            android:fontFamily="@font/pretendard_bold"
            android:paddingVertical="25dp"
            app:layout_constraintTop_toBottomOf="@id/bottom_sheet_12th_line"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/bottom_sheet_11th_cl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingHorizontal="24dp"
        app:layout_constraintTop_toBottomOf="@id/bottom_sheet_12th_cl">

        <View
            android:id="@+id/bottom_sheet_11th_line"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/gray_800"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="11기"
            android:textColor="@color/gray_100"
            android:textSize="18sp"
            android:fontFamily="@font/pretendard_bold"
            android:paddingVertical="25dp"
            app:layout_constraintTop_toBottomOf="@id/bottom_sheet_11th_line"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/bottom_sheet_generation_cancel_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="닫기"
        android:fontFamily="@font/pretendard_bold"
        android:textSize="18sp"
        android:textColor="@color/gray_50"
        android:textAlignment="center"
        android:paddingVertical="16dp"
        android:background="@drawable/login_round_5_o"
        android:backgroundTint="@color/main"
        android:layout_marginTop="23dp"
        android:layout_marginBottom="20dp"
        android:layout_marginHorizontal="24dp"
        app:layout_constraintTop_toBottomOf="@id/bottom_sheet_11th_cl"
        app:layout_constraintBottom_toBottomOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>